{
  "version": "1.0",
  "description": "Complex UI test - Game menu with multiple panels and elements",
  "commands": [
    {
      "action": "create-scene",
      "name": "GameMenuScene",
      "path": "Assets/Scenes",
      "type": "DefaultGameObjects",
      "addToBuild": true
    },
    {
      "action": "add-canvas",
      "name": "MainUICanvas",
      "scene": "GameMenuScene",
      "renderMode": "ScreenSpaceOverlay",
      "referenceWidth": 1920,
      "referenceHeight": 1080,
      "scaleMode": "ScaleWithScreenSize"
    },
    {
      "action": "add-panel",
      "name": "HeaderPanel",
      "parent": "MainUICanvas",
      "width": 1200,
      "height": 120,
      "anchor": "TopCenter",
      "position": [0, -60]
    },
    {
      "action": "add-text",
      "name": "GameTitle",
      "parent": "HeaderPanel",
      "text": "AWESOME GAME",
      "fontSize": 36,
      "width": 800,
      "height": 80,
      "anchor": "MiddleCenter",
      "color": "#FFFFFF"
    },
    {
      "action": "add-panel",
      "name": "MainMenuPanel",
      "parent": "MainUICanvas",
      "width": 500,
      "height": 400,
      "anchor": "MiddleCenter"
    },
    {
      "action": "add-button",
      "name": "PlayButton",
      "parent": "MainMenuPanel",
      "text": "PLAY GAME",
      "width": 300,
      "height": 60,
      "anchor": "MiddleCenter",
      "position": [0, 100]
    },
    {
      "action": "add-button",
      "name": "OptionsButton",
      "parent": "MainMenuPanel",
      "text": "OPTIONS",
      "width": 300,
      "height": 60,
      "anchor": "MiddleCenter",
      "position": [0, 20]
    },
    {
      "action": "add-button",
      "name": "CreditsButton",
      "parent": "MainMenuPanel",
      "text": "CREDITS",
      "width": 300,
      "height": 60,
      "anchor": "MiddleCenter",
      "position": [0, -60]
    },
    {
      "action": "add-button",
      "name": "ExitButton",
      "parent": "MainMenuPanel",
      "text": "EXIT",
      "width": 300,
      "height": 60,
      "anchor": "MiddleCenter",
      "position": [0, -140]
    },
    {
      "action": "add-panel",
      "name": "SideInfoPanel",
      "parent": "MainUICanvas",
      "width": 300,
      "height": 500,
      "anchor": "MiddleRight",
      "position": [-150, 0]
    },
    {
      "action": "add-text",
      "name": "InfoTitle",
      "parent": "SideInfoPanel",
      "text": "Game Info",
      "fontSize": 24,
      "width": 280,
      "height": 40,
      "anchor": "TopCenter",
      "position": [0, -20]
    },
    {
      "action": "add-text",
      "name": "VersionText",
      "parent": "SideInfoPanel",
      "text": "Version: 1.0.0",
      "fontSize": 16,
      "width": 280,
      "height": 30,
      "anchor": "TopCenter",
      "position": [0, -80]
    },
    {
      "action": "add-text",
      "name": "PlayerStatsText",
      "parent": "SideInfoPanel",
      "text": "High Score: 0\nGames Played: 0\nTime Played: 0h",
      "fontSize": 14,
      "width": 280,
      "height": 100,
      "anchor": "MiddleCenter",
      "position": [0, -50]
    },
    {
      "action": "add-panel",
      "name": "FooterPanel",
      "parent": "MainUICanvas",
      "width": 1200,
      "height": 80,
      "anchor": "BottomCenter",
      "position": [0, 40]
    },
    {
      "action": "add-text",
      "name": "CopyrightText",
      "parent": "FooterPanel",
      "text": "Â© 2025 Unity Vibe CLI - Generated with JSON Batch Commands",
      "fontSize": 12,
      "width": 1000,
      "height": 30,
      "anchor": "MiddleCenter",
      "color": "#CCCCCC"
    }
  ]
}